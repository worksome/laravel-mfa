{
    "name": "worksome/laravel-mfa",
    "description": "A driver-based multifactor authentication package for Laravel",
    "keywords": [
        "worksome",
        "laravel",
        "laravel-mfa"
    ],
    "homepage": "https://github.com/worksome/laravel-mfa",
    "license": "MIT",
    "authors": [
        {
            "name": "Owen Voke",
            "email": "owen.voke@worksome.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/contracts": "^8.73",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0 || ^2.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-parallel": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.26",
        "worksome/pest-plugin-silence": "^0.1",
        "worksome/coding-style": "0.15.0"
    },
    "autoload": {
        "psr-4": {
            "Worksome\\MultiFactorAuth\\": "src",
            "Worksome\\MultiFactorAuth\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Worksome\\MultiFactorAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": [
            "vendor/bin/php-cs-fixer fix --ansi",
            "vendor/bin/phpcbf"
        ],
        "test:unit": "vendor/bin/pest -p",
        "test:coverage": "vendor/bin/pest -p --coverage",
        "test:types": "vendor/bin/phpstan analyse",
        "test:style": [
            "vendor/bin/php-cs-fixer fix --dry-run --ansi",
            "vendor/bin/phpcs -n"
        ],
        "test": [
            "@test:style",
            "@test:types",
            "@test:unit"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "worksome/coding-style": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Worksome\\MultiFactorAuth\\TwoFactorAuthServiceProvider"
            ],
            "aliases": {
                "TwoFactorAuth": "Worksome\\MultiFactorAuth\\Facades\\TwoFactorAuth"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
